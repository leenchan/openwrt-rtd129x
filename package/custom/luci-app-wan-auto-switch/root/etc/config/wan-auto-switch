
config general
	option enable '0'
	option timeout '1'

config mode
	option enable '1'
	option name 'DHCP'
	option proto 'dhcp'

config mode
	option name 'ADSL'
	option proto 'pppoe'
	option username 'USERNAME'
	option password 'PASSWORD'
	option enable '1'

config mode
	option enable '0'
	option proto 'static'
	option ipv4_address '192.168.1.2'
	option ipv4_gateway '192.168.1.1'
	option name 'Static'

