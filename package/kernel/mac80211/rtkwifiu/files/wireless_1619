config wifi-device 'radio0'
        option type 'mac80211'
        option phy 'phy0'
        option channel '11'
        option hwmode '11g'
        option path 'platform/980a0000.pcie2/pci0001:01/0001:01:00.0'
        option htmode 'HT20'

config wifi-iface 'default_radio0'
        option device 'radio0'
        option network 'lan'
        option mode 'ap'
        option ssid 'OpenWrt_InitialSetup'
        option encryption 'none'

config wifi-device 'radio1'
        option type 'mac80211'
        option phy 'phy1'
        option channel '11'
        option hwmode '11a'
        option path 'platform/980a0000.pcie2/pci0001:01/0001:01:00.0'
        option htmode 'HT20'
        option disabled '1'

config wifi-iface 'default_radio1'
        option device 'radio1'
        option network 'lan'
        option mode 'ap'
        option ssid 'OpenWrt'
        option encryption 'none'
